{
  "compilerOptions": {
    "baseUrl": ".",
    "lib": [
      "ESNext",
      "dom"
    ],
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "jsx": "react"
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./src/web-client/**/*"
  ]
}